#include "CAN.h"
#include "stm32f765xx.h"


void CAN1_RX0_IRQHandler(void);
void init_can(CAN_TypeDef * CANx){
	CANx->MCR|=1; //request to enter initialization mode
	while(~CANx->MSR^CAN_MSR_INAK_Msk){;}//wait until hardware enters initialization mode
	CANx->BTR =& ~0x7F03FF;
	CANx->BTR =| 0x1e0001; //set for 1 Mb/s
	CANx->MCR|=&1;



}
void can_transmit(CAN_Message *msg);


