#include "timers.h"




//initalize timer
int timer_init(TIM_TypeDef* timx){
timx->PSC=215;
return (1);
}
void wait_for(int wait, TIM_TypeDef* timx){
	timx->ARR=wait*1000;
	timx->EGR|=0x1; //generate update event.
	timx->CR1=0x4; //enable in one pulse mode
}
